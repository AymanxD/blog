{
  "name": "blog",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/blog/src",
  "targets": {
    "dev": {
      "command": "astro dev",
      "options": {
        "cwd": "apps/blog"
      }
    },
    "build": {
      "command": "astro build",
      "options": {
        "cwd": "apps/blog"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "preview": {
      "command": "astro preview",
      "options": {
        "cwd": "apps/blog"
      },
      "dependsOn": ["build"]
    }
  }
}
